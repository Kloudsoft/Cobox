<!DOCTYPE html>

<html>
<head>
	<title>@this.ViewBag.Title</title>
	<link href="~/Styles/Common.css" rel="stylesheet" />
	<style type="text/css">
		.DivUserCurrent,
		.DivUserCurrent a,
		.DivUserCurrent a:hover,
		.DivUserCurrent a:visited
		{
			color: white;
		}
	</style>
	@Scripts.Render("~/bundles/jQuery");
    @Scripts.Render("~/bundles/PublicScripts");
	<script src="~/TypeScript/HouseOfSynergy.js"></script>
    <link rel="icon" type="image/png" href="~/Images/Favicon/32.png" />
</head>
<body>

	<div id="DivMain">
		<div id="DivHeader">
			<div id="DivHeaderLeft">
				<img src="~/Multimedia/Graphics/Images/Logo - Kloud-Soft.png" alt="Kloud-Soft" style="width: 300px; height: 100px;" />
			</div>
			<div id="DivHeaderMiddle">
				Affinity DMS
			</div>
			<div id="DivHeaderRight">
				<div class="DivUserCurrent">
					<a href="/Tenants/SignIn" title="Sign in to your account.">Sign In</a>
					<br />
					<a href="/Home" title="Head back to the main product website.">Product</a>
				</div>
			</div>
		</div>
		<div id="DivNavigation">
			Section Menus
		</div>
		<div id="DivMenuMain">
			<a href="/Tenants/Services" title="Services"><div class="DivMenuMainItem">Services</div></a>
			<a href="/Tenants/Solutions" title="Solutions"><div class="DivMenuMainItem">Solutions</div></a>
			<a href="/Tenants/About" , title="About"><div class="DivMenuMainItem">About</div></a>
			<a href="/Tenants/Clients" title="Clients"><div class="DivMenuMainItem">Clients</div></a>
			<a href="/Tenants/WhyUs" title="Why Us?"><div class="DivMenuMainItem">WhyUs</div></a>
			<a href="/Tenants/Help" title="Help"><div class="DivMenuMainItem">Help</div></a>
		</div>
		<div id="DivBody">
			@RenderBody()
		</div>
		<div id="DivFooter">
			Copyright © 2016 Kloud-Soft Pte Ltd. All rights reserved.
		</div>
	</div>

</body>
</html>