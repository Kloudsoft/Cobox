<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>@this.ViewBag.Title</title>
	<link href="~/Styles/Common.css" rel="stylesheet" />
	<style type="text/css">
		.DivUserCurrent,
		.DivUserCurrent a,
		.DivUserCurrent a:hover,
		.DivUserCurrent a:visited
		{
			color: white;
		}
	</style>
</head>
<body>

	<div id="DivMain">
		<div id="DivHeader">
			<div id="DivHeaderLeft">
				<img src="~/Multimedia/Graphics/Images/Logo - Kloud-Soft.png" alt="Kloud-Soft" style="width: 300px; height: 100px;" />
			</div>
			<div id="DivHeaderMiddle">
				Affinity DMS
			</div>
			<div id="DivHeaderRight">
				<div class="DivUserCurrent">
					<a href="/Master/SignIn" title="Sign in to your account.">Sign In</a>
					<br />
					<a href="/Home" title="Head back to the main product website.">Product</a>
				</div>
			</div>
		</div>
		<div id="DivNavigation">
			Section Menus
		</div>
		<div id="DivMenuMain">
			<a href="/Master/Help" title="Help"><div class="DivMenuMainItem">Help</div></a>
		</div>
		<div id="DivBody">
			@RenderBody()
		</div>
		<div id="DivFooter">
			Copyright © 2016 Kloud-Soft Pte Ltd. All rights reserved.
		</div>
	</div>

	<script src="~/Scripts/jquery-2.2.1.js"></script>
	<script src="~/Scripts/respond.js"></script>
	<script src="~/Scripts/modernizr-2.8.3.js"></script>
	<script src="~/Scripts/bootstrap.js"></script>

</body>
</html>